{
	"env": {
		"es6": true,
		"browser": true
	},
	"extends": [
		"eslint:recommend",
		"plugin:react/recommended",
		"airbnb",
		"prettier"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"plugins": ["react", "@typescript-eslint", "prettier"],
	"rules": {}
}
